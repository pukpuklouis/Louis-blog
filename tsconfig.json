{
  "extends": "astro/tsconfigs/base",
  "include": [".astro/types.d.ts", "**/*"],
  "exclude": [
    "dist",
    "public/~partytown"
  ],
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@/*": [
        "./src/*"
      ]
    },
    "jsxImportSource": "react",
    "jsx": "react-jsx",
    "plugins": [
      {
        "name": "@astrojs/ts-plugin"
      }
    ]
  }
}